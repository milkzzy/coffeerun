{
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "jsx-a11y"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            "warn",
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "no-console": "off"
    }
}